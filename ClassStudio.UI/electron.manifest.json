{
  "executable": "ClassStudio.UI",
  "splashscreen": {
    "imageFile": ""
  },
  "singleInstance": false,
  "build": {
    "appId": "com.ClassStudio.app",
    "productName": "ClassStudio",
    "copyright": "Copyright © 2019 - João Neves (shivayl)",
    "buildVersion": "0.5.0",
    "compression": "maximum",
    "directories": {
      "output": "../../../bin/Desktop"
    },
    "extraResources": [
      {
        "from": "./bin",
        "to": "bin",
        "filter": [ "**/*" ]
      }
    ],
    "files": [
      {
        "from": "./ElectronHostHook/node_modules",
        "to": "ElectronHostHook/node_modules",
        "filter": [ "**/*" ]
      },
      "**/*"
    ]
  },
  "settings": {
    "githubVersionFileUrl": "https://raw.githubusercontent.com/joao-neves95/ClassStudio/master/VERSION.md"
  }
}
