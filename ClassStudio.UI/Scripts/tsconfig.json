{
  "compilerOptions": {
    "alwaysStrict": true,
    "lib": [ "es6", "dom" ],
    "module": "system",
    "newLine": "LF",
    "noEmit": false,
    "noEmitOnError": false,
    "noImplicitAny": false,
    "outFile": "../wwwroot/js/app.js",
    "pretty": true,
    "removeComments": true,
    "sourceMap": true,
    "strict": true,
    "target": "es6"
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}
